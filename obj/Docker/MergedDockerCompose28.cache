name: dockercompose4750629031559795243
services:
  apigateway:
    build:
      context: F:\Asp.net\eShopMicroservices
      dockerfile: ApiGateway/Dockerfile
    command:
      - dotnet
      - ApiGateway.dll
    container_name: apigateway
    depends_on:
      eureka-server:
        condition: service_started
        required: true
      orderapi:
        condition: service_started
        required: true
      productapi:
        condition: service_started
        required: true
      userapi:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:80
      Eureka__Client__ServiceUrl__DefaultZone: http://eureka-server:8761/eureka/
      OcelotBaseUrl: http://localhost:8001
    hostname: apigateway
    image: apigateway
    networks:
      backend: null
    ports:
      - mode: ingress
        target: 80
        published: "8001"
        protocol: tcp
      - mode: ingress
        target: 80
        protocol: tcp
    restart: on-failure
  eureka-server:
    container_name: eureka-server
    environment:
      EUREKA_SERVER_ENABLE_SELF_PRESERVATION: "false"
    hostname: eureka-server
    image: steeltoeoss/eurekaserver:latest
    networks:
      backend: null
    ports:
      - mode: ingress
        target: 8761
        published: "8761"
        protocol: tcp
    restart: on-failure
  orderapi:
