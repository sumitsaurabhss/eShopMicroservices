{
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": "true",
      "registryFetchIntervalSeconds": 5,
      "EurekaServer": {
        "ConnectTimeoutSeconds": 5
      },
      "Health": {
        "Enabled": true,
        "CheckEnabled": true
      }
    },
    "instance": {
      "hostName": "localhost",
      "leaseRenewalIntervalInSeconds": 5,
      "lease-expiration-duration-in-seconds": 5
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Steeltoe": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ApiSettings": {
    "Secret": "microservices assignment secret key",
    "Issuer": "userapi",
    "Audience": "client"
  }
}
